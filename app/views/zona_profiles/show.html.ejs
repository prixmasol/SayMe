<div class="hero-unit">
  <h2 class="znNombre"><%= zonaProfile.znNombre %></h2>
  <%- linkTo('Edit this zonaProfile', editZonaProfilePath(params.id), {class: 'btn pull-right'}); %>
  <%- linkTo('Back to list', zonaProfilePath(''), {class: 'btn pull-right'}); %>
</div>

<h2>ZonaProfile Properties</h2>
<% for(var i in zonaProfile) { -%>
<% if(i !== 'saved') { -%>
<div class="row list-item">
  <div class="span4">
    <h3><%= i %></h3>
  </div>
  <div class="span8">
    <h3><i class="icon-list-alt"></i><span class="<%= i %>"><%= zonaProfile[i] %></span></h3>
  </div>
</div>
<% } -%><% } -%>