<div class="hero-unit">
  <h2>All ZonaProfiles</h2>
  <%- linkTo('Create a new ZonaProfile', addZonaProfilePath, {class: 'btn pull-right'}); %>
</div>

<div id="zonaProfiles-list">
<% if (zonaProfiles) { %>
<% for (var i = 0, ii = zonaProfiles.length; i < ii; i++) { %>
  <div class="row list-item" id="zonaProfile-<%= zonaProfiles[i].id; %>">
    <div class="span8">
      <h3><%- linkTo(zonaProfiles[i].znNombre, zonaProfilePath(zonaProfiles[i].id)); %></h3>
    </div>
    <div class="span4">
      <h3><i class="icon-list-alt"></i><%= zonaProfiles[i].id; %></h3>
    </div>
  </div>
<% } %>
<% } %>
</div>