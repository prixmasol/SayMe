<div class="hero-unit">
  <h2 class="potNombre"><%= potencialProfile.potNombre %></h2>
  <%- linkTo('Edit this potencialProfile', editPotencialProfilePath(params.id), {class: 'btn pull-right'}); %>
  <%- linkTo('Back to list', potencialProfilePath(''), {class: 'btn pull-right'}); %>
</div>

<h2>PotencialProfile Properties</h2>
<% for(var i in potencialProfile) { -%>
<% if(i !== 'saved') { -%>
<div class="row list-item">
  <div class="span4">
    <h3><%= i %></h3>
  </div>
  <div class="span8">
    <h3><i class="icon-list-alt"></i><span class="<%= i %>"><%= potencialProfile[i] %></span></h3>
  </div>
</div>
<% } -%><% } -%>