<div class="hero-unit">
  <h2>All PotencialProfiles</h2>
  <%- linkTo('Create a new PotencialProfile', addPotencialProfilePath, {class: 'btn pull-right'}); %>
</div>

<div id="potencialProfiles-list">
<% if (potencialProfiles) { %>
<% for (var i = 0, ii = potencialProfiles.length; i < ii; i++) { %>
  <div class="row list-item" id="potencialProfile-<%= potencialProfiles[i].id; %>">
    <div class="span8">
      <h3><%- linkTo(potencialProfiles[i].id, potencialProfilePath(potencialProfiles[i].id)); %></h3>
    </div>
    <div class="span4">
      <h3><i class="icon-list-alt"></i><%= potencialProfiles[i].id; %></h3>
    </div>
  </div>
<% } %>
<% } %>
</div>