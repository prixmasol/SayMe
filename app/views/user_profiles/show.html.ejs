<div class="hero-unit">
  <h2 class="upUserName"><%= userProfile.upUserName %></h2>
  <%- linkTo('Edit this userProfile', editUserProfilePath(params.id), {class: 'btn pull-right'}); %>
  <%- linkTo('Back to list', userProfilePath(''), {class: 'btn pull-right'}); %>
</div>

<h2>UserProfile Properties</h2>
<% for(var i in userProfile) { -%>
<% if(i !== 'saved') { -%>
<div class="row list-item">
  <div class="span4">
    <h3><%= i %></h3>
  </div>
  <div class="span8">
    <h3><i class="icon-list-alt"></i><span class="<%= i %>"><%= userProfile[i] %></span></h3>
  </div>
</div>
<% } -%><% } -%>