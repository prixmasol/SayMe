<div class="hero-unit">
  <h2>All ProfesionalTimeTables</h2>
  <%- linkTo('Create a new ProfesionalTimeTable', addProfesionalTimeTablePath, {class: 'btn pull-right'}); %>
</div>

<div id="profesionalTimeTables-list">
<% if (profesionalTimeTables) { %>
<% for (var i = 0, ii = profesionalTimeTables.length; i < ii; i++) { %>
  <div class="row list-item" id="profesionalTimeTable-<%= profesionalTimeTables[i].id; %>">
    <div class="span8">
      <h3><%- linkTo(profesionalTimeTables[i].pttDia, profesionalTimeTablePath(profesionalTimeTables[i].id)); %></h3>
    </div>
    <div class="span4">
      <h3><i class="icon-list-alt"></i><%= profesionalTimeTables[i].id; %></h3>
    </div>
  </div>
<% } %>
<% } %>
</div>