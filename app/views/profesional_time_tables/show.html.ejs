<div class="hero-unit">
  <h2 class="pttDia"><%= profesionalTimeTable.pttDia %></h2>
  <%- linkTo('Edit this profesionalTimeTable', editProfesionalTimeTablePath(params.id), {class: 'btn pull-right'}); %>
  <%- linkTo('Back to list', profesionalTimeTablePath(''), {class: 'btn pull-right'}); %>
</div>

<h2>ProfesionalTimeTable Properties</h2>
<% for(var i in profesionalTimeTable) { -%>
<% if(i !== 'saved') { -%>
<div class="row list-item">
  <div class="span4">
    <h3><%= i %></h3>
  </div>
  <div class="span8">
    <h3><i class="icon-list-alt"></i><span class="<%= i %>"><%= profesionalTimeTable[i] %></span></h3>
  </div>
</div>
<% } -%><% } -%>