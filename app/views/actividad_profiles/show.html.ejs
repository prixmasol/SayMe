<div class="hero-unit">
  <h2 class="actNombre"><%= actividadProfile.actNombre %></h2>
  <%- linkTo('Edit this actividadProfile', editActividadProfilePath(params.id), {class: 'btn pull-right'}); %>
  <%- linkTo('Back to list', actividadProfilePath(''), {class: 'btn pull-right'}); %>
</div>

<h2>ActividadProfile Properties</h2>
<% for(var i in actividadProfile) { -%>
<% if(i !== 'saved') { -%>
<div class="row list-item">
  <div class="span4">
    <h3><%= i %></h3>
  </div>
  <div class="span8">
    <h3><i class="icon-list-alt"></i><span class="<%= i %>"><%= actividadProfile[i] %></span></h3>
  </div>
</div>
<% } -%><% } -%>