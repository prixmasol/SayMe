<div class="hero-unit">
  <h2>All EntidadProfiles</h2>
  <%- linkTo('Create a new EntidadProfile', addEntidadProfilePath, {class: 'btn pull-right'}); %>
</div>

<div id="entidadProfiles-list">
<% if (entidadProfiles) { %>
<% for (var i = 0, ii = entidadProfiles.length; i < ii; i++) { %>
  <div class="row list-item" id="entidadProfile-<%= entidadProfiles[i].id; %>">
    <div class="span8">
      <h3><%- linkTo(entidadProfiles[i].entNombre, entidadProfilePath(entidadProfiles[i].id)); %></h3>
    </div>
    <div class="span4">
      <h3><i class="icon-list-alt"></i><%= entidadProfiles[i].id; %></h3>
    </div>
  </div>
<% } %>
<% } %>
</div>