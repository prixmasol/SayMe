<div class="control-group">
  <label for="ipsNombre" class="control-label">Nombre</label>
  <div class="controls">
    <%- contentTag('input', ipsProfile.ipsNombre, {type:'text', class:'span6', name:'ipsNombre'}) %>
  </div>
</div>
<div class="control-group">
  <label for="ipsIPSID" class="control-label">IPSID</label>
  <div class="controls">
    <%- contentTag('input', ipsProfile.ipsIPSID, {type:'text', class:'span6', name:'ipsIPSID'}) %>
  </div>
</div>
<div class="control-group">
  <label for="ipsDescripcion" class="control-label">Descripcion</label>
  <div class="controls">
    <%- contentTag('textarea', ipsProfile.ipsDescripcion, {class: 'span6', name: 'ipsDescripcion', rows: 10}) %>
  </div>
</div>
<div class="control-group">
  <label for="ipsEstado" class="control-label">Estado</label>
  <div class="controls">
    <%- selectTag(['Activo','Inactivo'], ipsProfile.ipsEstado, {type:'text', class:'span6', name:'ipsEstado'}) %>
  </div>
</div>
