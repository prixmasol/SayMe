<div class="hero-unit">
  <h2 class="ipsNombre"><%= ipsProfile.ipsNombre %></h2>
  <%- linkTo('Edit this ipsProfile', editIpsProfilePath(params.id), {class: 'btn pull-right'}); %>
  <%- linkTo('Back to list', ipsProfilePath(''), {class: 'btn pull-right'}); %>
</div>

<h2>IpsProfile Properties</h2>
<% for(var i in ipsProfile) { -%>
<% if(i !== 'saved') { -%>
<div class="row list-item">
  <div class="span4">
    <h3><%= i %></h3>
  </div>
  <div class="span8">
    <h3><i class="icon-list-alt"></i><span class="<%= i %>"><%= ipsProfile[i] %></span></h3>
  </div>
</div>
<% } -%><% } -%>