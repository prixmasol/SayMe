<div class="hero-unit">
  <h2 class="mPNombre"><%= maestroProfile.mPNombre %></h2>
  <%- linkTo('Edit this maestroProfile', editMaestroProfilePath(params.id), {class: 'btn pull-right'}); %>
  <%- linkTo('Back to list', maestroProfilePath(''), {class: 'btn pull-right'}); %>
</div>

<h2>MaestroProfile Properties</h2>
<% for(var i in maestroProfile) { -%>
<% if(i !== 'saved') { -%>
<div class="row list-item">
  <div class="span4">
    <h3><%= i %></h3>
  </div>
  <div class="span8">
    <h3><i class="icon-list-alt"></i><span class="<%= i %>"><%= maestroProfile[i] %></span></h3>
  </div>
</div>
<% } -%><% } -%>